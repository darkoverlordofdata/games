{
  "name": "Game",
  "base": "PersistedModel",
  "strict": false,
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "number",
      "id": true,
      "required": true,
      "index": true,
      "comments": "id"
    },
    "active": {
      "type": "boolean",
      "comments": "is active"
    },
    "name": {
      "type": "string",
      "comments": "game name"
    },
    "slug": {
      "type": "string",
      "comments": "url segment"
    },
    "url": {
      "type": "string",
      "comments": "full url"
    },
    "author": {
      "type": "string",
      "comments": "games author"
    },
    "description": {
      "type": "string",
      "comments": "description"
    },
    "version": {
      "type": "string",
      "comments": "app version"
    },
    "icon": {
      "type": "string",
      "comments": "app icon"
    },
    "main": {
      "type": "string",
      "comments": "html file to run"
    },
    "height": {
      "type": "string"
    },
    "width": {
      "type": "number",
      "comments": "window width"
    }
